</$objtype/mkfile

# to make it easier to play with multiple versions, compute the name
# of the binary from the current dir name
PWD=`{pwd}
TARG=`{basename $PWD}
OFILES=lx.$O procports.$O
BIN=/$objtype/bin
MAN=/sys/man
CFLAGS=-FTVw

</sys/src/cmd/mkone

myman:V:
	cp man/lx.1 /sys/man/1/$TARG
	cp man/lx.6 /sys/man/6/$TARG
